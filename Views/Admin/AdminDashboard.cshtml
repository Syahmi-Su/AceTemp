                    </ul>
                </li>
                <li class=" nav-item">
                    <a href="#"><i class="ft-credit-card"></i><span class="menu-title" data-i18n="">Outstanding</span></a>
                    <ul class="list-unstyled">
                        <li>
                            <a href="/Outstanding/Index">Manage <br />Outstanding <br />Payment</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!-- ///////////////////////////END SIDEBAR/////////////////////////////////////////////////-->

    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            <div class="content-header row">
            </div>
            <div class="content-body">
                <!-- Chart -->
                <div class="row match-height">
                    <div class="col-12">
                        <div class="">
                            <!--<div id="gradient-line-chart1" class="height-250 GradientlineShadow1"></div>-->
                        </div>
                    </div>
                </div>
                <!-- Chart -->
                <!-- eCommerce statistic -->
                <div class="row">
                    <div class="col-xl-4 col-lg-6 col-md-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted danger position-absolute p-1">Total Primary Students</h5>
                                <div>
                                    <i class="la la-group danger font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3  ">
                                    <!--<div id="progress-stats-bar-chart"></div>
                                    <div id="progress-stats-line-chart" class="progress-stats-shadow"></div>-->
                                    <h1 class="font-large-3 float-right p-1" align="right">@primarystudents</h1>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-md-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted info position-absolute p-1">No. Outstanding Payment</h5>
                                <div>
                                    <i class="la la-leanpub info font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3">
                                    <h1 class="font-large-3 float-right p-1" align="right">@totalsubjects</h1>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted warning position-absolute p-1">No. Cleared Payment</h5>
                                <div>
                                    <i class="la la-money warning font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3">
                                    <h1 class="font-large-3 float-right p-1" align="right">@totaloutstandings</h1>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/ eCommerce statistic -->
                <!-- eCommerce statistic -->
                <div class="row">
                    <div class="col-xl-4 col-lg-6 col-md-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted danger position-absolute p-1">Total Secondary Students</h5>
                                <div>
                                    <i class="la la-group danger font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3  ">
                                    <!--<div id="progress-stats-bar-chart"></div>
                                    <div id="progress-stats-line-chart" class="progress-stats-shadow"></div>-->
                                    <h1 class="font-large-3 float-right p-1" align="right">@secondarystudents</h1>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-lg-6 col-md-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted info position-absolute p-1">Unpaid Fee</h5>
                                <div>
                                    <i class="la la-files-o info font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3">
                                    <br /><br />
                                    <h4 class="font-large-3 float-right p-1" align="right">RM @totalpackages</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div href="/Outstanding/Index" class="col-xl-4 col-lg-12">
                        <div class="card pull-up ecom-card-1 bg-white">
                            <div class="card-content ecom-card2 height-180">
                                <h5 class="text-muted warning position-absolute p-1">Total Collected Payment This Month</h5>
                                <div>
                                    <i class="ft-credit-card warning font-large-1 float-right p-1"></i>
                                </div>
                                <div class="progress-stats-container ct-golden-section height-75 position-relative pt-3">
                                    <br /><br />
                                    <h4 class="font-large-3 float-right p-1" align="right">RM @pendingpayment</h4>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="content-body">
                    <!-- Basic Tables start -->
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title">Outstanding Payment List</h4>
                                    <a class="heading-elements-toggle"><i class="la la-ellipsis-v font-medium-3"></i></a>
                                    <div class="heading-elements">
                                        <ul class="list-inline mb-0">
                                            <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                                            <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                                            <li><a data-action="expand"><i class="ft-maximize"></i></a></li>

                                        </ul>
                                    </div>
                                </div>

                                <div class="card-header">
                                    <button class="btn btn-success btn-sm" onclick="javascript: fnExcelReport('dataTable', 'data')" style="float: right;"><i class="la la-file-excel-o"></i>&nbsp; Download</button>
                                </div>
                                <div class="card-content collapse show">
                                    <div class="card-body">
                                        <p class="card-text"></p>
                                        <p><span class="text-bold-600"></span></p>
                                        <div class="table-responsive">
                                            <table id="dataTable" class="table table-bordered table-hover display" style="width:100%">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>Month Issued</th>
                                                        <th>Parent IC</th>
                                                        <th>Outstanding Fees</th>
                                                        <th>Remark</th>
                                                        <th>Status</th>
@*                                                        <th>Action</th>*@
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @if (Model == null)
                                                    {
                                                    <tr>
                                                        <td colspan="9"><div style="text-align: center">No record Found</div></td>
                                                    </tr>
                                                    }
                                                    else
                                                    {
                                                       foreach (var outstanding in Model)
                                                       {

                                                                <tr>
                                                                    <td>@outstanding.outstandingdetails.O_ID</td>
                                                                    <td>@outstanding.outstandingdetails.O_month.ToString("MMMM / yyyy")</td>
                                                                    <td>@outstanding.outstandingdetails.O_pID</td>
                                                                    <td>@outstanding.outstandingdetails.O_fees</td>
                                                                    <td>@outstanding.outstandingdetails.O_remark</td>
                                                                    <td>@outstanding.statusdetails.status_desc</td>
@*                                                                    <td>
                                                                        @Html.ActionLink(" Approval", "ApprovalOutstanding", new { id = outstanding.outstandingdetails.O_ID }, new { @class = "btn btn-warning fas fa-trash-alt" })
                                                                        &nbsp;
                                                                        @Html.ActionLink(" Send Email", "SendOutstandingEmail", new { id = outstanding.outstandingdetails.O_ID }, new { @class = "btn btn-light fas fa-trash-alt" })
                                                                    </td>*@
                                                                </tr>

                                                       }
                                                      
                                                    }

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
                <!--Try-->
            </div>
        </div>


        <!-- ///////////////////////////START FOOTER/////////////////////////////////////////////////-->


        <footer class="footer footer-static footer-light navbar-border navbar-shadow">
            <div class="clearfix blue-grey lighten-2 text-sm-center mb-0 px-2">
                <p class="mt-50 mx-auto footer-text col-lg-12">
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | Pusat Tuisyen Madani
                </p>

            </div>
        </footer>

        <!-- ///////////////////////////END FOOTER/////////////////////////////////////////////////-->
        <!-- BEGIN VENDOR JS-->
        <script src="~/Content/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
        <!-- BEGIN VENDOR JS-->
        <!-- BEGIN PAGE VENDOR JS-->
        <script src="~/Content/theme-assets/vendors/js/charts/chartist.min.js" type="text/javascript"></script>
        <!-- END PAGE VENDOR JS-->
        <!-- BEGIN CHAMELEON  JS-->
        <script src="~/Content/theme-assets/js/core/app-menu-lite.js" type="text/javascript"></script>
        <script src="~/Content/theme-assets/js/core/app-lite.js" type="text/javascript"></script>
        <!-- END CHAMELEON  JS-->
        <!-- BEGIN PAGE LEVEL JS-->
        <script src="~/Content/theme-assets/js/scripts/pages/dashboard-lite.js" type="text/javascript"></script>
        <!-- END PAGE LEVEL JS-->

</body>
</html>


