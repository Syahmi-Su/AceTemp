                                    <ul class="list-inline mb-0">
                                        <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                                        <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                                        <li><a data-action="expand"><i class="ft-maximize"></i></a></li>

                                    </ul>
                                </div>
                            </div>

                            <div class="card-header">
                                <button class="btn btn-success btn-sm" onclick="javascript: fnExcelReport('dataTable', 'data')" style="float: right;"><i class="la la-file-excel-o"></i>&nbsp; Download</button>
                            </div>
                            <div class="card-content collapse show">
                                <div class="card-body">
                                    <button style="float:left;" class="btn btn-info btn-sm" onclick="location.href='@Url.Action("AddSubject", "AddSubject")';return false;"><i class="ft-plus"></i>&nbsp;Add Subject</button>&nbsp;&nbsp;
                                    <br /><br />
                                    <p class="card-text"></p>
                                    <p><span class="text-bold-600"></span></p>
                                    <div class="table-responsive">
                                        <table id="dataTable" class="table table-bordered table-hover display" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Subject Code</th>
                                                    <th>Student Name</th>
                                                    <th>Subject Type</th>
                                                    <th>Subject Year</th>
                                                    <th>Action</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var subject in Model)
                                                {

                                                    <tr>
                                                        <td>@subject.subject_code</td>
                                                        <td>@subject.subject_name</td>
                                                        <td>@subject.subject_type</td>
                                                        <td>@subject.subject_year</td>

                                                        <th>
                                                            @Html.ActionLink(" Edit", "EditSubjectDetails", new { id = subject.subject_code }, new { @class = "btn btn-warning fas fa-edit" })
                                                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                            @Html.ActionLink(" Delete", "DeleteSubject", new { id = subject.subject_code }, new { @class = "btn btn-danger fas fa-trash-alt" })
                                                        </th>


                                                    </tr>

                                                }

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
