        <div class="content-page">

            <!-- Start content -->
            <div class="content">

                <div class="container-fluid">

                    <div class="row">
                        <div class="col-xl-12">
                            <div class="breadcrumb-holder">
                                <h1 class="main-title float-left">Outstanding List</h1>
                                <ol class="breadcrumb float-right">
                                    <li class="breadcrumb-item">Home</li>
                                    <li class="breadcrumb-item active">Outstanding List</li>
                                </ol>
                                <div class="clearfix"></div>
                            </div>


                        </div>
                    </div>
                    <!-- end row -->

                    <div class="row">
                        <div class="col-12">
                            <div class="card mb-3">
                                <div class="card-header">
                                    <h3><i class="fas fa-user-friends"></i> Outstanding</h3>
                                    <button style="float:right;position:relative;" class="btn btn-info btn-sm" onclick="location.href='@Url.Action("Create", "Outstanding")';return false;"><i class="fa fa-plus"></i>&nbsp;Add Outstanding</button>
                                </div>

                                <div class="card-body">

                                    <div class="table-responsive">
                                        <table id="dataTable" class="table table-bordered table-hover display" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>ID</th>
                                                    <th>Month</th>
                                                    <th>Parent</th>
                                                    <th>Fees</th>
                                                    <th>Remark</th>
                                                    <th>Status</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var outstanding in Model)
                                                {

                                                    <tr>
                                                        <td>@outstanding.O_ID</td>
                                                        <td>@outstanding.O_month</td>
                                                        <td>@outstanding.O_pID</td>
                                                        <td>@outstanding.O_fees</td>
                                                        <td>@outstanding.O_remark</td>
                                                        <td>@outstanding.O_status</td>
                                                        <td>
                                                            <a class='btn btn-info' href='@Url.Action("Edit", "Outstanding", new { id = outstanding.O_ID})'>Edit</a>
                                                            &nbsp;&nbsp;&nbsp;
                                                            <a class='btn btn-info' href='@Url.Action("Details", "Outstanding", new { id = outstanding.O_ID})'>Details</a>
                                                            &nbsp;&nbsp;&nbsp;
                                                            <a class='btn btn-info' href='@Url.Action("Delete", "Outstanding", new { id = outstanding.O_ID})'>Delete</a>
                                                        </td>
                                                    </tr>

                                                }

                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- end table-responsive-->
                                    <div>

                                    </div>
                                </div>
                                <!-- end card-body-->
                            </div>
                            <!-- end card-->
                        </div>


                    </div>
